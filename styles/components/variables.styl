fadeIn =
  transition opacity 1s linear, z-index 1s linear
  opacity 1
  z-index 20
fadeInPopup =
  transition opacity 1s linear, z-index 1s linear
  opacity 1
  z-index 100
fadeOut = 
  transition opacity 1s linear, z-index 1s linear
  opacity 0
  z-index -1  
unsupported = 
  position relative
  top 0px
  left 0px
  margin 10px
  background red
disabled =
  pointer-events none
  cursor default
  background-color #999
  opacity 0.5
